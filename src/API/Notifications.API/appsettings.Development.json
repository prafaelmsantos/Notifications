{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "GrpcServerPort": 9010,
  "HttpPort": 5010,
  "HttpsPort": 5011,
  "GrpcClient": {
    "Notifications": {
      "Url": "localhost:9010"
    },
    "BackOff": {
      "MaxAttempts": 5,
      "InitialBackoff": 1,
      "MaxBackoff": 5,
      "BackoffMultiplier": 1.5
    }
  }
}
